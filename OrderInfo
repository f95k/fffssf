		frame.getContentPane().add(lblDatum);
		
		JScrollPane scrollPane = new JScrollPane();
		scrollPane.setBounds(6, 171, 466, 142);
		frame.getContentPane().add(scrollPane);
		
		JList list = new JList();
		list.setModel(new AbstractListModel() {
			String[] values = new String[] {};
			public int getSize() {
				return values.length;
			}
			public Object getElementAt(int index) {
				return values[index];
			}
		});
		scrollPane.setViewportView(list);
		
		JLabel lblNewLabel = new JLabel("Best√§llda artiklar");
		lblNewLabel.setForeground(SystemColor.controlHighlight);
		lblNewLabel.setBounds(183, 152, 121, 16);
		frame.getContentPane().add(lblNewLabel);
		
		textField = new JTextField();
		textField.setBackground(SystemColor.window);
		textField.setEditable(false);
		textField.setBounds(170, 16, 134, 28);
		frame.getContentPane().add(textField);
		textField.setColumns(10);
		
		textField_1 = new JTextField();
		textField_1.setBackground(SystemColor.window);
		textField_1.setEditable(false);
		textField_1.setColumns(10);
		textField_1.setBounds(170, 50, 134, 28);
		frame.getContentPane().add(textField_1);
		
		textField_2 = new JTextField();
